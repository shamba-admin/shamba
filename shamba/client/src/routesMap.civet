{getProjects} from ./actions.civet

export VIEWS = {
  PROJECTS: "Projects",
}

thunkWithState := (f) => (dispatch, getState) => {
  state := getState();

  f(dispatch, state.location.payload, state);
}

export default {
  [VIEWS.PROJECTS]: {
    path: "/",
    thunk: thunkWithState((dispatch) => {
      dispatch(getProjects);
    }),
  },
}